@{
}

<h2>Member Detail</h2>


<table class="table">
    <tr>
        <td>Id</td><td>@ViewBag.member.Id</td>
    </tr>
    <tr>
        <td>Name</td>
        <td>@ViewBag.member.Name</td>
    </tr>
    <tr>
        <td>Email</td>
        <td>@ViewBag.member.Email</td>
    </tr>
    <tr>
        <td>Active</td>
        <td>@(ViewBag.member.Active ? "Active" : "Inactive")</td>
    </tr>
</table>
